{
  "name": "realcoin",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 7070",
    "buildExternal": "tsc server/db.ts server/server.ts --target es2022 --module es2022 --lib es2022 --moduleResolution node --esModuleInterop --skipLibCheck --forceConsistentCasingInFileNames --outDir serverOut --declaration --sourceMap",
    "build": "vite build && npm run buildExternal",
    "preview": "vite preview",
    "start": "node --env-file .env serverOut/server.js",
    "proxy": "node --env-file .env serverOut/server.js --proxy",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.544.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@tailwindcss/vite": "^4.1.13",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/http-proxy": "^1.17.16",
    "@types/speakeasy": "^2.0.10",
    "@types/ws": "^8.18.1",
    "bits-ui": "^2.11.4",
    "clsx": "^2.1.1",
    "layerchart": "^2.0.0-next.27",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.39.5",
    "svelte-check": "^4.3.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "author": "Tally",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^24.6.2",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "http-proxy": "^1.18.1",
    "mode-watcher": "^1.1.0",
    "speakeasy": "^2.0.0",
    "surrealdb": "^1.3.2",
    "ws": "^8.18.3",
    "zod": "^4.1.11"
  }
}
